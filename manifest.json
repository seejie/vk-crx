{
  "name": "vipkid",
  "version": "1.0",
  "manifest_version": 2,
  "description": "try to do something for better",
  "author": "liusijie@vipkid.com.cn",
  "icons": {
		"16": "images/logo.png",
		"48": "images/logo.png",
		"128": "images/logo.png"
	},
  "browser_action": {
    "default_icon": "/images/logo.png",
    "default_title": "have a nice coding",
    "default_popup": "/page/popup.html"
  },
  "content_scripts": [{
    "matches": [
      "http://wiki.vipkid.com.cn/pages/resumedraft.action*",
      "http://wiki.vipkid.com.cn/pages/viewpage.action*"
    ],
    "js": ["js/base.js", "js/weeklyReport2.js"]
  }, {
    "matches": ["http://*.vipkid-inc.com/Sys/auth/Index"],
    "js": ["js/base.js", "js/runNowBtn.js"]
  }, {
    "matches": ["https://code.vipkid.com.cn/*"],
    "js": ["js/base.js", "js/gitlab.js"]
  }],
  "background": {
    "scripts": ["global/background.js"]
  },
  // cannot overrides more than one page
  "chrome_url_overrides": {
    // "newtab": "/page/newTab.html"
    // "history": "/page/history.html"
	  // "bookmarks": "/page/bookmarks.html"
  },
  "devtools_page": "devtools.html",
  "permissions": [
    "notifications",
    "storage",
    "https://github.com/"
  ]
}
