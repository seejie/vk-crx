{
  "name": "vipkid",
  "version": "1.0",
  "manifest_version": 2,
  "description": "try to do something for better",
  "browser_action": {
    "default_icon": "/images/logo.png",
    "default_title": "have a nice coding",
    "default_popup": "/page/popup.html"
  },
  "content_scripts": [{
    "matches": ["http://wiki.vipkid.com.cn/pages/resumedraft.action*"],
    "js": ["js/base.js", "js/weeklyReport.js"]
  }, {
    "matches": ["http://*.vipkid-inc.com/Sys/auth/Index"],
    "js": ["js/base.js", "js/runNowBtn.js"]
  }, {
    "matches": ["https://code.vipkid.com.cn/*"],
    "js": ["js/base.js", "js/gitlab.js"]
  }],
  "background":{
    "scripts": ["global/background.js"]
  },
  "permissions": [
    "notifications",
    "https://github.com/"
  ]
}
