{
  "name": "vipkid",
  "version": "1.0",
  "manifest_version": 2,
  "description": "try to do something for better",
  "browser_action": {
    "default_icon": "./images/logo.png",
    "default_title": "have a nice coding",
    "default_popup": "index.html"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["js/base.js", "js/weeklyReport.js", "js/runNowBtn.js", "js/gitlab.js", "js/index.js"],
      "css": ["static/css/index.css"],
      "run_at": "document_start"
    }
  ],
  "background":{
    "scripts": ["global/background.js"]
  },
  "permissions": [
    "notifications"
  ]
}
